<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>


    <declare-styleable name="SilkySignaturePad">
        <attr name="penMinWidth" format="dimension" />
        <attr name="penMaxWidth" format="dimension" />
        <attr name="penColor" format="color" />
        <attr name="velocityFilterWeight" format="float" />
        <attr name="clearOnDoubleClick" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="CoverView">
        <attr name="horizontalPadding"  format="dimension"/>
        <attr name="verticalPadding"    format="dimension"/>
        <attr name="backgroundColor"    format="color"/>
    </declare-styleable>

    <declare-styleable name="RectView">
        <attr name="x1"             format="integer"/>
        <attr name="y1"             format="integer"/>
        <attr name="x2"             format="integer"/>
        <attr name="y2"             format="integer"/>
        <attr name="confidence"     format="float"/>
        <attr name="color"          format="color"/>
        <attr name="textSize"       format="dimension"/>
        <attr name="textPadding"    format="dimension"/>
        <attr name="radius"         format="dimension" />
        <attr name="lineLength"     format="dimension" />
    </declare-styleable>



    <declare-styleable name="CropImageView">
        <attr name="cropGuidelines">
            <enum name="off" value="0"/>
            <enum name="onTouch" value="1"/>
            <enum name="on" value="2"/>
        </attr>
        <attr name="cropScaleType">
            <enum name="fitCenter" value="0"/>
            <enum name="center" value="1"/>
            <enum name="centerCrop" value="2"/>
            <enum name="centerInside" value="3"/>
        </attr>
        <attr name="cropShape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
        <attr name="cropAutoZoomEnabled" format="boolean"/>
        <attr name="cropMaxZoom" format="integer"/>
        <attr name="cropMultiTouchEnabled" format="boolean"/>
        <attr name="cropFixAspectRatio" format="boolean"/>
        <attr name="cropAspectRatioX" format="integer"/>
        <attr name="cropAspectRatioY" format="integer"/>
        <attr name="cropInitialCropWindowPaddingRatio" format="float"/>
        <attr name="cropBorderLineThickness" format="dimension"/>
        <attr name="cropBorderLineColor" format="color"/>
        <attr name="cropBorderCornerThickness" format="dimension"/>
        <attr name="cropBorderCornerOffset" format="dimension"/>
        <attr name="cropBorderCornerLength" format="dimension"/>
        <attr name="cropBorderCornerColor" format="color"/>
        <attr name="cropGuidelinesThickness" format="dimension"/>
        <attr name="cropGuidelinesColor" format="color"/>
        <attr name="cropBackgroundColor" format="color"/>
        <attr name="cropSnapRadius" format="dimension"/>
        <attr name="cropTouchRadius" format="dimension"/>
        <attr name="cropSaveBitmapToInstanceState" format="boolean"/>
        <attr name="cropShowCropOverlay" format="boolean"/>
        <attr name="cropShowProgressBar" format="boolean"/>
        <attr name="cropMinCropWindowWidth" format="dimension"/>
        <attr name="cropMinCropWindowHeight" format="dimension"/>
        <attr name="cropMinCropResultWidthPX" format="float"/>
        <attr name="cropMinCropResultHeightPX" format="float"/>
        <attr name="cropMaxCropResultWidthPX" format="float"/>
        <attr name="cropMaxCropResultHeightPX" format="float"/>
        <attr name="cropFlipHorizontally" format="boolean"/>
        <attr name="cropFlipVertically" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="StateProgressBar">

        <attr name="spb_stateBackgroundColor" format="color" />
        <attr name="spb_stateForegroundColor" format="color" />

        <attr name="spb_stateNumberBackgroundColor" format="color" />
        <attr name="spb_stateNumberForegroundColor" format="color" />

        <attr name="spb_currentStateDescriptionColor" format="color" />
        <attr name="spb_stateDescriptionColor" format="color" />

        <attr name="spb_stateSize" format="dimension" />

        <attr name="spb_stateTextSize" format="dimension" />

        <attr name="spb_stateDescriptionSize" format="dimension" />

        <attr name="spb_stateLineThickness" format="dimension" />

        <attr name="spb_checkStateCompleted" format="boolean" />

        <attr name="spb_stateNumberIsDescending" format="boolean" />

        <attr name="spb_animateToCurrentProgressState" format="boolean" />

        <attr name="spb_enableAllStatesCompleted" format="boolean" />

        <attr name="spb_descriptionTopSpaceDecrementer" format="dimension" />

        <attr name="spb_descriptionTopSpaceIncrementer" format="dimension" />

        <attr name="spb_animationDuration" format="integer" />

        <attr name="spb_animationStartDelay" format="integer" />

        <attr name="spb_justifyMultilineDescription" format="boolean" />

        <attr name="spb_descriptionLinesSpacing" format="dimension" />

        <attr name="spb_maxDescriptionLines" format="integer" />


        <attr name="spb_currentStateNumber" format="enum">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>


        <attr name="spb_maxStateNumber" format="enum">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>

    </declare-styleable>



    <declare-styleable name="SmartMaterialSpinner">
        <attr name="smsp_typeface" format="string" />
        <attr name="smsp_baseColor" format="color|reference" />
        <attr name="smsp_highlightColor" format="color|reference" />

        <attr name="smsp_itemView" format="reference" />
        <attr name="smsp_dropdownView" format="reference" />
        <attr name="smsp_searchDropdownView" format="reference" />

        <attr name="smsp_isSearchable" format="boolean" />
        <attr name="smsp_showKeyboardOnStart" format="boolean" />
        <attr name="smsp_enableSearchHeader" format="boolean" />
        <attr name="smsp_searchHeaderText" format="string" />
        <attr name="smsp_searchHeaderTextColor" format="color|reference" />
        <attr name="smsp_searchHeaderBackgroundColor" format="color|reference" />
        <attr name="smsp_searchHint" format="string" />
        <attr name="smsp_searchHintColor" format="color" />
        <attr name="smsp_searchTextColor" format="color" />
        <attr name="smsp_searchFilterColor" format="color" />
        <attr name="smsp_searchBackgroundColor" format="color|reference" />
        <attr name="smsp_enableDismissSearch" format="boolean" />
        <attr name="smsp_dismissSearchText" format="string" />
        <attr name="smsp_dismissSearchColor" format="color" />

        <attr name="smsp_itemSize" format="dimension" />
        <attr name="smsp_itemColor" format="color|reference" />

        <attr name="smsp_autoSelectable" format="boolean" />
        <attr name="smsp_showDropdownHint" format="boolean" />
        <attr name="smsp_hint" format="string" />
        <attr name="smsp_hintSize" format="dimension" />
        <attr name="smsp_hintColor" format="color|reference" />

        <attr name="smsp_itemListColor" format="color|reference" />
        <attr name="smsp_selectedItemListColor" format="color|reference" />

        <!--<attr name="smsp_showItemListHint" format="boolean" />-->
        <attr name="smsp_itemListHintColor" format="color|reference" />
        <attr name="smsp_itemListHintBackgroundColor" format="color|reference" />
        <attr name="smsp_itemListBackgroundColor" format="color|reference" />

        <attr name="smsp_enableFloatingLabel" format="boolean" />
        <attr name="smsp_alwaysShowFloatingLabel" format="boolean" />
        <attr name="smsp_floatingLabelText" format="string" />
        <attr name="smsp_floatingLabelSize" format="dimension" />
        <attr name="smsp_floatingLabelColor" format="color|reference" />

        <attr name="smsp_arrowSize" format="dimension" />
        <attr name="smsp_arrowColor" format="color|reference" />
        <attr name="smsp_arrowPaddingLeft" format="dimension" />
        <attr name="smsp_arrowPaddingTop" format="dimension" />
        <attr name="smsp_arrowPaddingRight" format="dimension" />
        <attr name="smsp_arrowPaddingBottom" format="dimension" />
        <attr name="smsp_paddingLeftRight" format="dimension" />

        <attr name="smsp_underlineSize" format="dimension" />
        <attr name="smsp_underlineColor" format="color|reference" />

        <attr name="smsp_enableErrorLabel" format="boolean" />
        <attr name="smsp_errorText" format="string" />
        <attr name="smsp_errorTextSize" format="dimension" />
        <attr name="smsp_errorTextColor" format="color|reference" />
        <attr name="smsp_multilineError" format="boolean" />
        <attr name="smsp_nbErrorLine" format="integer" />
        <attr name="smsp_errorTextAlignment" format="enum">
            <enum name="align_left" value="0" />
            <enum name="align_center" value="1" />
            <enum name="align_right" value="2" />
        </attr>

        <attr name="smsp_alignLabel" format="boolean" />
        <attr name="smsp_isRtl" format="boolean" />
        <!--<attr name="smsp_enableDefaultSelect" format="boolean" />-->
        <attr name="smsp_isReSelectable" format="boolean" />

        <attr name="smsp_isOutlined" format="boolean" />
        <attr name="smsp_outlinedBoxRadius" format="dimension" />
        <attr name="smsp_outlinedHintStartX" format="dimension" />
        <attr name="smsp_outlinedHintPadding" format="dimension" />
        <attr name="smsp_outlinedStrokeWidth" format="dimension" />
        <attr name="smsp_outlinedBoxColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ExpandableCardView">
        <attr name="title" format="string"/>
        <attr name="icon" format="reference"/>
        <attr name="inner_view" format="reference"/>
        <attr name="expandOnClick" format="boolean"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="startExpanded" format="boolean"/>
    </declare-styleable>




</resources>
